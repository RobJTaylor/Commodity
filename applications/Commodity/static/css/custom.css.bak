/*custom css*/
.reviewerImage {
    width: 20vw;
    height: auto;
    max-width: 300px;
}

.custombold {
    font-weight: bold;
}

.helpfulText a{
    color: #000000;
    font-weight: bold;
}

.center {
    text-align: center;
    margin-left: auto;
    margin-right: auto;
}

.reviewText {
    font-weight: bold;
    display: inline-block;
}

.reviews {
    margin-left: 20px;
    width: 90%;
}
.jumotronlogo {
    margin-top: 15px;
    margin-bottom: 15px;
}

a {
    color: #000000;
    text-decoration: underline;
}
.newProducts {
    color: #31343a;
    text-align: center;
    margin-bottom: 20px;
}

.productLeft {
    float: left;
}
.productRight {
    float: right;
    text-align: left;
    margin-left: 0px;
}
.largeimage {
    width: 100%;
    height: auto;
    max-width: 550px;
}
.hdimage {
    width: auto;
    height: 15vh;
    padding-top: 15px;
    padding-bottom: 15px;
}
.hdbgcolor {
    background-color: #31343a;
    overflow: hidden;
}
.bgcolor-green {
    background-color: #93c54b;
    overflow: hidden;
    color: #efefef;
}
.fill-left-right {
    width: 100%;
}
.rounded-corners {
    border-radius: 5px;
}
.textcolor {
    color: #dbdde0;
}
.tagtext {
    color: #dbdde0;
    font-size: 10px;
}
.text-top {
    margin-top: -20px;
}
